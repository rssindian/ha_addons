{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Start Home Assistant",
      "type": "shell",
      "command": "supervisor_run",
      "group": {
        "kind": "test",
        "isDefault": true
      },
      "presentation": {
        "reveal": "always",
        "panel": "new"
      },
      "problemMatcher": []
    },
    {
      "type": "shell",
      "label": "Copy to HA Addon Local",
      "command": "cp",
      "args": [
          "-recurse",
          "-force",
          "${workspaceFolder}/vpnjackett/",
          "Z:/vpnjackett/"
      ],
      "presentation": {
        "reveal": "always",
        "panel": "new"
      },
      "problemMatcher": []
    }
  ]
}
